{
  "$schema": "vscode://schemas/color-theme",
  "name": "JetBrains Dark Vibrant",
  "include": "./jetbrains-dark.json",
  "tokenColors": [
    {
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#16BAAC"
      }
    },
    {
      "name": "[Python] Attribute Caps Enum & Constant",
      "scope": [
        "constant.other.caps.python",
      ],
      "settings": {
        "foreground": "#C77DBB"
      }
    },
    {
      "name": "[Python] storage.type.string",
      "scope": [
        "storage.type.string.python",
      ],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
    // {
    //   "name": "[Rust] Type Parameter",
    //   "scope": [
    //     "entity.name.type.rust",
    //   ],
    //   "settings": {
    //     "foreground": "#16BAAC"
    //   }
    // },
    {
      "name": "[Vue] directive",
      "scope": [
        "meta.attribute.directive.vue entity.other.attribute-name.html.vue",
        "meta.attribute.directive.vue punctuation.attribute-shorthand.event.html.vue",
        "meta.attribute.directive.vue punctuation.attribute-shorthand.bind.html.vue",
        "punctuation.attribute-shorthand.slot.html.vue"
      ],
      "settings": {
        "foreground": "#CF8E6D"
      }
    },
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#16BAAC",
    "struct": "#16BAAC",
    // "class:python": "#2fbaa3",
    "class.typeHint": "#8888C6",
    // "variable.readonly": "#C77DBB",
    "enum": "#16BAAC",
    "enum.defaultLibrary": "#8888C6",
    // "namespace": "#16BAAC",
    "module": "#16BAAC",
    "interface": "#16BAAC"
  }
}